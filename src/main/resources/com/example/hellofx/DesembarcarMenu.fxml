<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="267.0" prefWidth="614.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hellofx.Main">
   <children>
      <Label layoutX="217.0" layoutY="29.0" text="Desembarcar vagão">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Label layoutX="42.0" layoutY="98.0" text="Informe o identificador do vagão que você deseja remover (Letra de a-z)">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <TextField fx:id="indexVagaoDesembarcar" layoutX="233.0" layoutY="134.0" onAction="#desembarcarFX" promptText="Digite uma letra de A-Z" />
      <Button fx:id="desembarcarSubmit" layoutX="264.0" layoutY="171.0" mnemonicParsing="false" onAction="#desembarcarFX" text="Desembarcar" />
   </children>
</AnchorPane>
